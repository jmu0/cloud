 #!/bin/sh
 #/etc/init.d/example
  
case "$1" in
    start)
        echo "Starting Cloud..."
        # run application you want to start
        # python3 -u `which cloud` run >> /var/log/cloud.log &
        cloud run >> /var/log/cloud.log &
        ;;
    stop)
        echo "Stopping Cloud..."
        # kill application you want to stop
        killall cloud
        ;;
    *)
        echo "Usage: /etc/init.d/cloud{start|stop}"
        exit 1
        ;;
esac

exit 0
